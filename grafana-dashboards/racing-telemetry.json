{
  "title": "ðŸ Sim Racing Telemetry",
  "tags": ["racing", "telemetry", "performance"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "1s",
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Speed (km/h)",
      "type": "timeseries",
      "datasource": "InfluxDB",
      "gridPos": {"x": 0, "y": 0, "w": 8, "h": 8},
      "targets": [{
        "query": "from(bucket: \"telemetry\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"telemetry\")\n  |> filter(fn: (r) => r[\"_field\"] == \"speed\")",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "km/h",
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never",
            "axisPlacement": "auto"
          },
          "unit": "velocitykmh"
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 2,
      "title": "RPM",
      "type": "timeseries",
      "datasource": "InfluxDB",
      "gridPos": {"x": 8, "y": 0, "w": 8, "h": 8},
      "targets": [{
        "query": "from(bucket: \"telemetry\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"telemetry\")\n  |> filter(fn: (r) => r[\"_field\"] == \"rpm\")",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "RPM",
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never"
          },
          "unit": "rpm"
        }
      }
    },
    {
      "id": 3,
      "title": "Current Speed",
      "type": "gauge",
      "datasource": "InfluxDB",
      "gridPos": {"x": 16, "y": 0, "w": 4, "h": 4},
      "targets": [{
        "query": "from(bucket: \"telemetry\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"telemetry\")\n  |> filter(fn: (r) => r[\"_field\"] == \"speed\")\n  |> last()",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 200, "color": "yellow"},
              {"value": 280, "color": "red"}
            ]
          },
          "unit": "velocitykmh",
          "min": 0,
          "max": 350
        }
      }
    },
    {
      "id": 4,
      "title": "Current RPM",
      "type": "gauge",
      "datasource": "InfluxDB",
      "gridPos": {"x": 20, "y": 0, "w": 4, "h": 4},
